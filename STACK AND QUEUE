#include<iostream>
#include<stack>
using namespace std;
int main(){
    stack <int> s;
    s.push(90);
    s.push(88);
    s.push(23);
    s.push(55);
    cout<<s.empty()<<endl;
    cout<<s.size()<<endl;
}

@#$
#include<iostream>
#include<stack>
using namespace std;
void printElementOFStack(stack <int> s){
    while(!s.empty()){
        cout<<s.top()<<endl;
        s.pop();
    }
}
int main(){
    stack <int> s;
    s.push(90);
    s.push(88);
    s.push(23);
    s.push(55);
    cout<<s.empty()<<endl;
    cout<<s.size()<<endl;
    printElementOFStack(s);
    
}


@#$#QUEUE
include<iostream>
#include<queue>
using namespace std;
void showdetails(queue<int> q){
    while(!q.empty()){
        cout<<q.front()<<endl;
        q.pop();
    }
}
int main(){
    queue <int> q;
    q.push(90);
    q.push(88);
    q.push(23);
    q.push(55);
    cout<<q.empty()<<endl;
    cout<<q.size()<<endl;
    showdetails(q);
    
    
}

@#@#Queue
#include<iostream>
#include<queue>
using namespace std;
void showdetails(queue<int> q){
    while(!q.empty()){
        cout<<q.front()<<endl;
        q.pop();
    }
}
int main(){
    queue <int> q;
    q.push(90);
    q.push(88);
    q.push(23);
    q.push(55);
    cout<<q.empty()<<endl;
    cout<<q.size()<<endl;
    showdetails(q);
    cout<<q.front()<<endl;
    cout<<q.back()<<endl;
    
    
}
