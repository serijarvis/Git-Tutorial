#include<iostream>
#include<algorithm>
using namespace std;
void showArray(int a[]){
    for(int i=0;i<10;i++){
        cout<<a[i]<<endl;
    }
    cout<<endl;
}
int main()
{
    int a[10]={9,8,4,5,6,1,2,89,56,34};
    showArray(a);
    sort(a,a+10);
    cout<<a<<endl;
    showArray(a);
}
@#$%%$#@#$%^
#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
void showArray(int a[]){
    for(int i=0;i<10;i++){
        cout<<a[i]<<endl;
    }
    cout<<endl;
}
int main()
{
    int a[10]={9,8,4,5,6,1,2,89,56,34};
    showArray(a);
    sort(a,a+10);
    cout<<a<<endl;
    showArray(a);
    vector <int> v;
    v.push_back(90);
    v.push_back(23);
    v.push_back(77);
    v.push_back(21);
    v.push_back(88);
    sort(v.begin(),v.end());
    for(int i=0;i<v.size();i++){
        cout<<v[i]<<endl;
    }
    cout<<endl;
}
