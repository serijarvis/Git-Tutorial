
#include <iostream>
#include<vector>

using namespace std;

int main()
{
    vector <int> v;
    v.push_back(10);
    v.push_back(20);
    for(auto it=v.begin();it!=v.end();it++)
    cout<<*it<<endl;
    

    return 0;
}
3433#$##$##
#include <iostream>
#include<vector>

using namespace std;

int main()
{
    vector <int> v;
    v.push_back(10);
    v.push_back(20);
    cout<<v.size()<<endl;
    for(auto it=v.begin();it!=v.end();it++)
    cout<<*it<<endl;
    for(int i=0;i<v.size();i++)
    cout<<v[i]<<endl;
    

    return 0;
}
@#$max_size can be stored in var
#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector <int> v;
    v.assign(5,19);
    v.push_back(10);
    v.push_back(20);
    // v.assign(5,2);
    // auto<<v.size()<<endl;
    for(auto it=v.begin();it!=v.end();it++)
    cout<<*it<<endl;
    cout<<"vector is empty or not"<<v.empty()<<endl;
    cout<<"the ,mamxin can be tore in "<<v.max_size()<<endl;
}
#$##insert function
include<iostream>
#include<vector>
using namespace std;
int main(){
    vector <int> v;
    v.assign(5,19);
    v.push_back(10);
    v.push_back(20);
    // v.assign(5,2);
    // auto<<v.size()<<endl;
    v.insert(v.begin(),-3);
    for(auto it=v.begin();it!=v.end();it++)
    cout<<*it<<endl;
    cout<<"vector is empty or not"<<v.empty()<<endl;
    cout<<"the ,mamxin can be tore in "<<v.max_size()<<endl;
}
@#

#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector <int> v;
    v.assign(5,19);
    v.push_back(10);
    v.push_back(20);
    // v.assign(5,2);
    // auto<<v.size()<<endl;
    v.insert(v.begin(),-3);
     v.insert(v.begin()+3,-3);
    for(auto it=v.begin();it!=v.end();it++)
    cout<<*it<<endl;
    cout<<"vector is empty or not"<<v.empty()<<endl;
    cout<<"the ,mamxin can be tore in "<<v.max_size()<<endl;
}
##erase

#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector <int> v;
    v.assign(5,19);
    v.push_back(10);
    v.push_back(20);
    // v.assign(5,2);
    // auto<<v.size()<<endl;
    v.insert(v.begin(),-3);
     v.insert(v.begin()+3,-3);
     v.erase(v.begin());
    for(auto it=v.begin();it!=v.end();it++)
    cout<<*it<<endl;
    cout<<"vector is empty or not"<<v.empty()<<endl;
    cout<<"the ,mamxin can be tore in "<<v.max_size()<<endl;
}


