#include <iostream>
#include<deque> 

using namespace std;
void showdetails(deque <int> q){
    deque <int> :: iterator it;
    for(it=q.begin();it!=q.end();it++){
        cout<<*it<<endl;
    }
    cout<<endl;
}

int main()
{
    deque <int> dq;
    dq.push_back(12);
    dq.push_front(90);
    dq.push_back(19);
    dq.push_front(99);
    dq.push_back(113);
    dq.push_front(900);
    cout<<"Hello World";
    showdetails(dq);
    cout<<"size of deque is"<<dq.size()<<endl;
    cout<<"size of deque is"<<dq.max_size()<<endl;
    cout<<dq.at(1)<<endl;
    cout<<dq.front()<<endl;
    cout<<dq.back()<<endl;
    dq.pop_back();
    showdetails(dq);
    dq.pop_front();
    showdetails(dq);

    return 0;
}
!@#$%
#include<iostream>
#include<queue> 
using namespace std;

void showpriorityqueue(priority_queue<int> pq){
    priority_queue <int> q=pq;
    while(!q.empty()){
        cout<<q.top()<<endl;
        q.pop();
    }
    cout<<endl;
}

int main(){
    priority_queue<int> q;
    q.push(10);
    q.push(20);
    q.push(5);
    showpriorityqueue(q);
    
}
@#$@#$
#include<iostream>
#include<queue> 
using namespace std;

void showpriorityqueue(priority_queue<int> pq){
    priority_queue <int> q=pq;
    while(!q.empty()){
        cout<<q.top()<<endl;
        q.pop();
    }
    cout<<endl;
}

int main(){
    priority_queue<int> q;
    q.push(10);
    q.push(20);
    q.push(5);
    q.push(900);
    showpriorityqueue(q);
    cout<<q.size()<<endl;
    q.pop();
    q.pop();
    showpriorityqueue(q);
    
    
}
