#include<iostream>
using namespace std;
int main(){
	int n;
	cin>>n;
	int i=1;
//	char ch='A';
	while(i<=n){
		int j=1;
		while(j<=i){
			char ch='A'+j+i-2;
			cout<<ch<<" ";
			
			j=j+1;
		}
		cout<<endl;
		i=i+1;
	}
	
}
