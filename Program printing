10>>>
#include<iostream>
using namespace std;
int main(){
	int n;
	cin>>n;
	int i=1;
	while(i<=n){
		int j=1;
		while(j<=i){
			cout<<(n-j+1)<<" ";
			j=j+1;
		}
		cout<<endl;
		i=i+1;
	}
}
output.,,;,..
6
6
6 5
6 5 4
6 5 4 3
6 5 4 3 2
6 5 4 3 2 1
