19>>>
#include<iostream>
using namespace std;
int main(){
	int n;
	cin>>n;
	int row=1;
	while(row<=n){
		int column=1;
		int count=1;
		while(column<=row){
			cout<<count;
			count=count+1;
			column=column+1;
		}
		cout<<endl;
		row=row+1;
	}
}
output"::???>>>
5
1
12
123
1234
12345
20>>
//here below code represents i =row and  j=column
#include<iostream>
using namespace std;
int main(){
	int n;
	cin>>n;
	int i=1;
//	char ch='A';
	while(i<=n){
		int j=1;
		while(j<=n){
			cout<<char(65+i-1)<<" ";
			
			j=j+1;
		}
		cout<<endl;
		i=i+1;
	}
	
}
output
6
A A A A A A
B B B B B B
C C C C C C
D D D D D D
E E E E E E
F F F F F F
54>>
//here below code represents i =row and  j=column
#include<iostream>
using namespace std;
int main(){
	int n;
	cin>>n;
	int i=1;
//	char ch='A';
	while(i<=n){
		int j=1;
		while(j<=n){
			char ch='A'+j-1;
			cout<<ch<<" ";
			
			j=j+1;
		}
		cout<<endl;
		i=i+1;
	}
	
}
output
6
A B C D E F
A B C D E F
A B C D E F
A B C D E F
A B C D E F
A B C D E F
22>>@##
//here below code represents i =row and  j=column
#include<iostream>
using namespace std;
int main(){
	int n;
	cin>>n;
	int i=1;
//	char ch='A';
	while(i<=n){
		int j=1;
		while(j<=n){
			char ch='A'+j+i-2;
			cout<<ch<<" ";
			
			j=j+1;
		}
		cout<<endl;
		i=i+1;
	}
	
}
output>>>
5
A B C D E
B C D E F
C D E F G
D E F G H
E F G H I
