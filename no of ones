#include <iostream>

using namespace std;

int main()
{
    int n;
    cout<<"enter the number"<<endl;
    cin>>n;
    int count=1;
    int x=n & (n-1);
    while(x!=0){
        count++;
        x=x & (x-1);
    }
    cout<<count<<endl;
    
    return 0;
}
