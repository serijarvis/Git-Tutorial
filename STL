#include<iostream>
#include<algorithm>
#include<vector>
using namespace std;
int main(){
    vector <int> v;
    vector <int> :: iterator it;
    v.push_back(10);
    v.push_back(8);
    v.push_back(78);
    v.push_back(90);
    v.pop_back();
    auto a=v.begin();
    cout<<"first element is "<<a[0]<<endl;
    int size=(int)v.size();
    it=v.begin();
    cout<<"iterator points is "<<it[0]<<endl;
    it=v.end();
    cout<<"iterator points is "<<it[0]<<endl;
    for(int i=0;i<size;i++){
        cout<<v[i]<<endl;
    }
}
