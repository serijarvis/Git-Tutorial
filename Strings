@#$@#$find function
#include<iostream>
using namespace std;
int main(){
    string s1="nincompoop";
    cout<<s1.find("poop")<<endl;
    cout<<s1.find("p")<<endl;
    cout<<s1.find("TRUE")<<endl;
    
}
