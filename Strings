}

@#@## stoi function
#include<iostream>
using namespace std;
int main(){
    string s1="786";
    int x=stoi(s1);
    cout<<x+2<<endl;
    
    
}

@#@#integer to strings
#include<iostream>
using namespace std;
int main(){
    int x=786;
    cout<<to_string(x)+"2"<<endl;
    
    
}

@#$@#@##@# sorting a string 
#include<iostream>
#include<algorithm>
using namespace std;
int main(){
    string s="ygecmlkkoiwqsawaaczdrroj";
    sort(s.begin(),s.end());
    cout<<s<<endl;
    
    
}

#include<iostream>
#include<algorithm>
using namespace std;
int main(){
    string s="ygecmlkkoiwqs3456789awaaczdrroj";
    sort(s.begin(),s.end());
    cout<<s<<endl;
    
    
}




