@#@####iterate function
#include<iostream>
using namespace std;
int main(){
    string s1="nincompoop";
    for(int i=0;i<s1.length();i++){
        cout<<s1[i]<<endl;
    }
    
}

@#@#@#substr
#include<iostream>
using namespace std;
int main(){
    string s1="nincompoop";
