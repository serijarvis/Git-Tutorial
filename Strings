}

23456!@#$%@##compare function 
#include<iostream>
using namespace std;
int main(){
    string s1="abc";
    string s2="xyz";
    cout<<s2.compare(s1)<<endl;
    cout<<s1.compare(s2)<<endl;
}
23456$$%%compare function
#include<iostream>
using namespace std;
int main(){
    string s1="abc";
    string s2="abc";
    cout<<s2.compare(s1)<<endl;
    cout<<s1.compare(s2)<<endl;
}
#$%$$%%%strings are equal
#include<iostream>
using namespace std;
int main(){
    string s1="abc";
    string s2="abc";
    if(s2.compare(s1)==0)
    cout<<"strings are equal"<<endl;
    cout<<s2.compare(s1)<<endl;
    cout<<s1.compare(s2)<<endl;
    cout<<"strings are equal"<<endl;
}

2#$%^45555E
Empty function
#include<iostream>
using namespace std;
int main(){
    string s1="abc";
    cout<<s1<<endl;
    s1.clear();
    if(s1.empty())
    cout<<"string are empty"<<endl;
}

@#$%^EMPTY
#include<iostream>
using namespace std;
int main(){
    string s1="abc";
    cout<<s1<<endl;
    s1.clear();
    if(!s1.empty())
    cout<<"string are empty"<<endl;
}
@#$@#$erase function 
#include<iostream>
using namespace std;
int main(){
    string s1="abcxyzabc";
    s1.erase(3,3);
    s1.erase(3,2);
    cout<<s1<<endl;
    
}

