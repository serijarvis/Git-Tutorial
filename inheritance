class Employee:
    company="amazon"
    def showdetails(self):
        print("this is an employee working in amazon")
class Programmer(Employee):
    langauage="java"
    def showdetails(self):
        print("this is an programmer working in youtube")
    def status(self):
        print(f"this is a language is {self.langauage}")

################

arun=Employee()
rohit=Programmer()
rohit.showdetails()
arun.showdetails()
rohit.status()
# arun.status()


class Freelancer:
    company="fiver"
    level=10
    def details(self):
        print(f"the company name is {self.company}")
        ################
        class Freelancer:
    company="fiver"
    level=10
    def details(self):
        print(f"the company name is {self.company}")

class Employee:
    company="flipkart"
    ecode=102
    def details(self):
        print(f"the company name is {self.company}  ")


