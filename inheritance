@##@############
class Employee:
    company="visa"
    ecode=102
class freelancer:
    company="flipkart"
    level=0

    def upgradelevel(self):
        self.level = self.level + 1
class Programmer(Employee,freelancer):
    name="rohit"
p=Programmer()
p.upgradelevel()
print(p.level)
print(p.company)
