v# A Python 3 program to put
# all negative numbers before
# positive numbers

def rearrange(arr, n ) :

	# Please refer partition() in
	# below post
	# https://www.geeksforgeeks.org / quick-sort / j = 0
	j = 0
	for i in range(0, n) :
		if (arr[i] < 0) :
			temp = arr[i]
			arr[i] = arr[j]
			arr[j]= temp
			j = j + 1
	print(arr)

# Driver code
arr = [-1, 2,-9, -3, 4, 5, 6, -7, 8, 9]
n = len(arr)
rearrange(arr, n)
####problem statement 10
Range of the given function
import numpy
arr=numpy.array([-1,-3,3,47,21,91])
max_element=numpy.max(arr)
min_element=numpy.min(arr)
print("the maximumu min_element",max_element)
print("the min elemnt is ",min_element)
range=(max_element-min_element)
print("so the range of the given program is ",range)
