{
    int i,arr[10];
    cout<<"enter 10 elements of the array";
    for(int i=0;i<10;i++)
    cin>>arr[i];
    int sum=0;
    for(int i=0;i<10;i++)
    sum=sum+arr[i];
    cout<<"the sum of the array is"<<sum;
    return 0;
}
