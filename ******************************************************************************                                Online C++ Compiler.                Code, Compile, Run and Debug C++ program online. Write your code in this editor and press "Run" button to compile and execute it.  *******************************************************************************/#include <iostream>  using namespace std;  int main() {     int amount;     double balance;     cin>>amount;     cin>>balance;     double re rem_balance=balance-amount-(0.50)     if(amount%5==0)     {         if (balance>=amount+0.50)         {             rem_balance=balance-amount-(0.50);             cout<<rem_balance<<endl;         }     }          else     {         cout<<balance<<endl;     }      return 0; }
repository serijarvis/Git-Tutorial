#include <iostream>

using namespace std;

int main()
{
    int amount;
    double balance;
    cin>>amount;
    cin>>balance;
    double rem_balance;
    // rem_balance=balance-amount-(0.50)
    if(amount%5==0)
    {
        if (balance>=amount+0.50)
        {
            rem_balance=balance-amount-(0.50);
            cout<<rem_balance<<endl;
        }
    }
    
    else
    {
        cout<<balance<<endl;
    }

    return 0;
}
